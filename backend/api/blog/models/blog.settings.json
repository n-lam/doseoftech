{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "coverPhoto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "likedBy": {
      "via": "likes",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    }
  }
}
